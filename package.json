{
  "name": "docker-gen",
  "version": "1.0.0",
  "description": "Docker Micro Services generator",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "plop": "plop",
    "start": "rm -rf app && plop",
    "start:dev": "rm -rf app && ./node_modules/plop/src/plop.js"
  },
  "keywords": [
    "docker",
    "generator",
    "service"
  ],
  "author": "Kristian Mandrup",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.1",
    "plop": "tecla5/plop"
  },
  "dependencies": {
    "fs-extra": "^3.0.1",
    "level": "^1.6.0"
  },
  "babel": {
    "presets": ["env"],
    "plugins": ["transform-runtime"]
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 1,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register", "babel-polyfill"
    ],
    "babel": "inherit"
  }
}